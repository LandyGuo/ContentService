<uwsgi>
    <socket>:8081</socket>
    <daemonize>%(log_dir_base)s/contentservice/uwsgi.log</daemonize>
    <env>DJANGO_SETTINGS_MODULE=contentservice.settings</env>
    <module>django.core.handlers.wsgi:WSGIHandler()</module>
    <uid>baina</uid>
    <gid>nogroup</gid>
    <master/>
    <master-as-root/>
    <memory-report/>
    <limit-as>512</limit-as>
    <listen>128</listen>
	<harakiri>60</harakiri>
	<harakiri-verbose/>
	<workers>4</workers>
	<socket-timeout>10</socket-timeout>
</uwsgi>